{
    "name": "cuadra-ai/cuadra-ai-sdk",
    "description": "Build AI agents with your own data, accelerate time-to-value, and create solutions as unique as your business.",
    "type": "library",
    "keywords": [
        "model",
        "embed",
        "ai",
        "cuadraai"
    ],
    "homepage": "https://cuadra.ai/",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Cuadra AI",
            "email": "dan@cuadra.ai"
        }
    ],
    "support": {},
    "prefer-stable": false,
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "apimatic/unirest-php": "^4.0.6",
        "apimatic/core-interfaces": "~0.1.5",
        "apimatic/core": "~0.3.13"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phan/phan": "5.4.5"
    },
    "autoload": {
        "psr-4": {
            "CuadraAiLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CuadraAiLib\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint-src": "phpcs --standard=phpcs-ruleset.xml src/",
        "lint-src-fix": "phpcbf --standard=phpcs-ruleset.xml src/",
        "analyze": "phan --allow-polyfill-parser -p",
        "lint": [
            "@lint-src"
        ]
    }
}